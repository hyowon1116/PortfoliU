<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app_profile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.App_profile">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.App_profile.NoActionBar"/>

        <activity
            android:name=".SplashActivity"
            android:theme="@style/Theme.App_profile.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name=".SaveMemoActivity"/>
        <activity android:name=".DetailActivity"/>
        <activity android:name=".SaveMemoActivity_foreign"/>
        <activity android:name=".DetailActivity_foreign"/>
        <activity android:name=".SaveMemoActivity_school"/>
        <activity android:name=".DetailActivity_school"/>
        <activity android:name=".SaveMemoActivity_outschool"/>
        <activity android:name=".DetailActivity_outschool"/>
        <activity android:name=".SaveMemoActivity_competition"/>
        <activity android:name=".DetailActivity_competition"/>
        <activity android:name=".SaveMemoActivity_todo"/>
        <activity android:name=".DetailActivity_todo"/>
        <activity android:name=".SaveMemoActivity_timetable"/>



    </application>

</manifest>
